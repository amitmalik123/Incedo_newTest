<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->

		<!-- standard, dark -->

		<theme>dark</theme>
		<!-- document encoding -->
		<timelineEnabled>true</timelineEnabled>
		<!-- defaults to UTF-8 -->

		<encoding>UTF-8</encoding>
		<!-- protocol for script and stylesheets -->

		<!-- defaults to https -->

		<protocol>http</protocol>
		<!-- title of the document -->
		<documentTitle>WFI_Regression Report</documentTitle>
		<!-- report name - displayed at top-nav -->

		<reportName>WFI_Regression Report</reportName>
		<!-- location of charts in the test view -->

		 <!-- timestamp format -->
        <timeStampFormat>MMM dd, yyyy HH:mm:ss</timeStampFormat>

		<!-- top, bottom -->

		<testViewChartLocation>bottom</testViewChartLocation>
		<!-- custom javascript -->

		<scripts>

 <![CDATA[
            $(document).ready(function() {
                $document.getElementById("nav-dashboard").click();
                $(‘#parentWithoutNodes’).prop("checked", true).click();
                $(‘#test-count-setting > div.modal-footer > a’)[0].click();
            });
        ]]>
		</scripts>
		<!-- custom styles -->
		<styles>
<![CDATA[
]]>
		</styles>
	</configuration>
</extentreports>